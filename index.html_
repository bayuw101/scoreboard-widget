<!doctype html>
<html lang="en">
  <head>
    <meta charset="UTF-8" />
    <link rel="icon" type="image/png" href="/favicon.png" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>Vite + React + TS</title>
  </head>
  <body>
    <div id="arbiter-scoreboard"></div>
    <div id="arbiter-scoreboard-left"></div>

    <script src="/arbiter-scoreboard.iife.js"></script>
    <script>
      ArbiterScoreboard.init({
          target: "#arbiter-scoreboard",
          token: "98a48f99-aaa8-4c00-8285-d9eeb25aeb11",
          options: {
            header: "top",
            thirdColor: "#0054d5",
            secondaryColor: "#001c41",
            primaryColor: "#001c41",
          },
        });
        
        ArbiterScoreboard.init({
          target: "#arbiter-scoreboard-left",
          token: "98a48f99-aaa8-4c00-8285-d9eeb25aeb11",
          options: {
            header: "left",
            thirdColor: "#0054d5",
            secondaryColor: "#001c41",
            primaryColor: "#001c41",
          },
        });
    </script>
  </body>
</html>
